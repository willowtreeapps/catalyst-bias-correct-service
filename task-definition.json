{
  "executionRoleArn": "arn:aws:iam::<aws-account-id>:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/catalyst-bias-correct-taskdef-<environment>",
          "awslogs-region": "<aws-region>",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 9000
        }
      ],
      "cpu": 0,
      "secrets": [
        {
          "valueFrom": "arn:aws:ssm:<aws-region>:<aws-account-id>:parameter/PLAY_SECRET_KEY",
          "name": "PLAY_SECRET_KEY"
        }
      ],
      "memoryReservation": 512,
      "image": "<aws-account-id>.dkr.ecr.<aws-region>.amazonaws.com/bias-correct-service:latest",
      "name": "bias-correct-image"
    }
  ],
  "taskRoleArn": "arn:aws:iam::<aws-account-id>:role/ecsTaskExecutionRole",
  "family": "catalyst-bias-correct-taskdef-<environment>",
  "requiresCompatibilities": [
    "EC2"
  ]
}